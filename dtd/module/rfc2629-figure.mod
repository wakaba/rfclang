<!-- Figures

       artwork, figure, postamble, preamble

     This module provides markup to support figure embedding.
-->

<!-- figure ............................................ -->

<!ENTITY % rfc2629.figure.element "INCLUDE">
<![%rfc2629.figure.element;[
<!ENTITY % rfc2629.figure.content "(%rfc2629.preamble.qname;?,%rfc2629.artwork.qname;,%rfc2629.postamble.qname;?)">
<!ENTITY % rfc2629.figure.qname "figure">
<!ENTITY % rfc2629.figure.tagmin.start "-">
<!ENTITY % rfc2629.figure.tagmin.end "-">
<![%sgml.tag.minimizable;[
<!ENTITY % rfc2629.figure.tagmin "%rfc2629.figure.tagmin.start; %rfc2629.figure.tagmin.end;">
<!-- end of sgml.tag.minimizable -->]]>
<!ENTITY % rfc2629.figure.tagmin "">
<!ELEMENT %rfc2629.figure.qname; %rfc2629.figure.tagmin; %rfc2629.figure.content;>
<!-- end of rfc2629.figure.element -->]]>

<!ENTITY % rfc2629.figure.attlist "INCLUDE">
<![%rfc2629.figure.attlist;[
<!ATTLIST %rfc2629.figure.qname;
	%rfc2629.anchor.attrib;
	title	%rfc2629.ATEXT.datatype;	""
	%rfc2629.common.attrib;>
<!-- end of rfc2629.figure.attlist -->]]>

