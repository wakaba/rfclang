<!ENTITY % rfc2629ja.l.content "(%rfc2629.TEXT.datatype;|%Inline.mix;|%rfc2629.artwork.qname;|%rfc2629ja.artwork.qname;|%rfc2629.preamble.qname;|%rfc2629.postamble.qname;)*">
<!ENTITY % rfc2629ja.l.qname "l">
<!ENTITY % rfc2629ja.l.tagmin.start "-">
<!ENTITY % rfc2629ja.l.tagmin.end "-">
<![%sgml.tag.minimizable;[
<!ENTITY % rfc2629ja.l.tagmin "%rfc2629ja.l.tagmin.start; %rfc2629ja.l.tagmin.end;">
<!-- end of sgml.tag.minimizable -->]]>
<!ENTITY % rfc2629ja.l.tagmin "">
<!ELEMENT %rfc2629ja.l.qname; %rfc2629ja.l.tagmin; %rfc2629ja.l.content;>
<!-- end of rfc2629ja.l.element -->]]>

<!ENTITY % rfc2629ja.l.attlist "INCLUDE">
