<!ATTLIST %rfc2629ja.pair.qname;
	%rfc2629ja.common.attrib;>
<!-- end of rfc2629ja.pair.attlist -->]]>

<!-- l: can contain rfc2629[ja]:{artwork|{pre|post}amble} only
