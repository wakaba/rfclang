<![%rfc2629ja.pair.element;[
<!ENTITY % rfc2629ja.pair.content "(%rfc2629ja.l.qname;)+">
<!ENTITY % rfc2629ja.pair.qname "pair">
<!ENTITY % rfc2629ja.pair.tagmin.start "-">
<!ENTITY % rfc2629ja.pair.tagmin.end "-">
<![%sgml.tag.minimizable;[
<!ENTITY % rfc2629ja.pair.tagmin "%rfc2629ja.pair.tagmin.start; %rfc2629ja.pair.tagmin.end;">
<!-- end of sgml.tag.minimizable -->]]>
<!ENTITY % rfc2629ja.pair.tagmin "">
